#include <iostream>
using namespace std;
void update(int *a,int *b) {
    // Complete this function
    int c=*a;
    *a=*a+*b;
    *b=*a-*b-*b;
    if (*b < 0)
    {*b = *b * -1;}
}

int main() {
    int x,y;
    cin>>x>>y;
    update(&x,&y);
    cout<<x<<endl<<y<<endl;
    
    
    
    

    return 0;
}
